  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run eslint:fix",
      "npm run prettier:fix"
    ],
    "*.{js,css,md}": "npm run prettier:fix"
  }
